/context/Modal.js",[],"/home/luke/WSL/AppAcademy/dotify/frontend/src/index.js",[],["156","157"],"/home/luke/WSL/AppAcademy/dotify/frontend/src/App.js",[],"/home/luke/WSL/AppAcademy/dotify/frontend/src/store/csrf.js",[],"/home/luke/WSL/AppAcademy/dotify/frontend/src/store/session.js",[],"/home/luke/WSL/AppAcademy/dotify/frontend/src/context/Modal.js",[],"/home/luke/WSL/AppAcademy/dotify/frontend/src/store/index.js",[],"/home/luke/WSL/AppAcademy/dotify/frontend/src/components/Navigation/index.js",[],"/home/luke/WSL/AppAcademy/dotify/frontend/src/components/LoginFormModal/index.js",[],"/home/luke/WSL/AppAcademy/dotify/frontend/src/components/LoginFormModal/LoginForm.js",[],"/home/luke/WSL/AppAcademy/dotify/frontend/src/components/HomePage/index.js",[],"/home/luke/WSL/AppAcademy/dotify/frontend/src/components/SignupFormModal/index.js",[],"/home/luke/WSL/AppAcademy/dotify/frontend/src/components/SignupFormModal/SignupForm.js",[],"/home/luke/WSL/AppAcademy/dotify/frontend/src/components/PlayingNow/index.js",[],["158","159"],"/home/luke/WSL/AppAcademy/dotify/frontend/src/components/MusicBar/index.js",[],"/home/luke/WSL/AppAcademy/dotify/frontend/src/components/Sidebar/index.js",[],"/home/luke/WSL/AppAcademy/dotify/frontend/src/components/Navigation/ProfileButton.js",[],"/home/luke/WSL/AppAcademy/dotify/frontend/src/store/songs.js",["160","161"],"import { fetch } from './csrf';\nconst SET_SONGS = 'songs/SET_SONGS';\nconst REMOVE_SONG = 'songs/REMOVE_SONG'\n\n//action creater that sets the song in the song slice of state\nexport const setSongs = (songs) => {\n    return {\n        type: SET_SONGS,\n        payload: songs,\n    };\n};\n\nexport const removeSong = (songId) => {\n    return {\n        type: REMOVE_SONG,\n        payload: songId,\n    };\n}\n\n\nexport const deleteSongStore = (songId) => async (dispatch)=>{\n    const res = await fetch(`api/songs/${songId}`,{\n        method: \"DELETE\"\n    })\n    dispatch(removeSong(songId))\n}\n\n\n\n//thunk which accepts an object of key value pairs and turns them into FormData entries to send with your request.\nexport const createSong = (song) => async (dispatch) => {\n    const { name, artist, songFile, userId } = song; //can add songs entry if multiple\n\n    const formData = new FormData();\n    formData.append(\"name\", name);\n    formData.append(\"artist\", artist);\n    formData.append(\"songFile\", songFile);\n    formData.append(\"userId\", userId);\n\n    // for multiple files\n    // if (images && images.length !== 0) {\n    //     for (var i = 0; i < images.length; i++) {\n    //         formData.append(\"images\", images[i]);\n    //     }\n    // }\n\n    // for single file\n    const res = await fetch(`/api/songs/`, {\n        headers: {\n            \"Content-Type\": \"multipart/form-data\",\n        },\n        method: \"POST\",\n        body: formData,\n    });\n\n};\n\n\n\n\n//this is the thunk that\n// 1) gets songs from route\n// 2) normalizes song data\n// 3) updates state\n\nexport const getSongs = () => async dispatch => {\n    const res = await fetch('/api/songs');\n    const songs = res.data.songs\n    let normalizedSongs = {}\n    for (let i =0; i<songs.length; i++){\n        const song = songs[i]\n        normalizedSongs[song.id] = song\n    }\n    dispatch(setSongs(normalizedSongs))\n};\n\nconst initialState = {};\n\n//holds the current songs state information\nexport const songsReducer = (state = initialState, action) => {\n    let newState;\n    switch (action.type) {\n        case SET_SONGS:\n            newState = Object.assign({}, state, action.payload);\n            return newState;\n        case REMOVE_SONG:\n            newState = { ...state }\n            delete newState[action.payload]\n            return newState;\n        default:\n            return state;\n    }\n};\n","/home/luke/WSL/AppAcademy/dotify/frontend/src/components/AllSongs/index.js",["162","163","164","165"],"import React from 'react'\nimport { useSelector, useDispatch } from 'react-redux';\nimport { useEffect, useState } from 'react'\nimport * as songActions from \"../../store/songs\"\nimport * as likeActions from \"../../store/likes\"\nimport * as currentSongActions from \"../../store/currentSong\"\nimport * as likesActions from \"../../store/likes\"\nimport * as likedSongsActions from \"../../store/likedSongs\"\nimport \"./AllSongs.css\"\nimport { NavLink } from 'react-router-dom'\n\n\n\nfunction AllSongs() {\n\n    const dispatch = useDispatch();\n\n    const songs = useSelector((state) => Object.values(state.songs));\n    const sessionUser = useSelector((state) => state.session.user);\n    const userId = sessionUser.id\n\n    useEffect(() => {\n        dispatch(songActions.getSongs())\n        dispatch(likesActions.getLikes())\n        dispatch(likedSongsActions.getLikedSongs(userId))\n    }, [dispatch])\n\n\n    function likeSong(songId) {\n        dispatch(likeActions.createLike({ userId, songId }))\n        dispatch(likesActions.getLikes())\n    }\n    function unLikeSong(songId) {\n        dispatch(likeActions.deleteLikeStore({ userId, songId }))\n        dispatch(likesActions.getLikes())\n    }\n    function deleteSong(songId) {\n        dispatch(songActions.deleteSongStore(songId))\n    }\n\n\n    return (\n        <>\n            <div className=\"container\">\n                <div className=\"music\">\n\n                    <div className=\"all-songs\">\n                        {songs.map(song =>\n                        (<li className=\"song-lists\" key={song.id}>\n\n                            <h2 className=\"song-title\">{song.name}</h2>\n                            <p className=\"song-artist\">{song.artist}</p>\n                            <audio controls className=\"music-controls\">\n                                <source src={song.filePath} type=\"audio/mp3\" />\n                            </audio>\n                            <div className=\"options\">\n                                <button className=\"song-button\" value={song.id} onClick={() => likeSong(song.id)}>\n                                    <i className=\"fas fa-heart Like song-icon\" value={song.id}></i>\n                                </button>\n                                <button className=\"song-button\" value={song.id} onClick={() => unLikeSong(song.id)}>\n                                    <i className=\"fas fa-heart unLike song-icon\" value={song.id}></i>\n                                </button>\n                                <button className=\"song-button\" value={song.id} onClick={() => deleteSong(song.id)}>\n                                    <i className=\"fas fa-times removeSong song-icon\" value={song.id} ></i>\n                                </button>\n                            </div>\n\n                        </li>))\n                        }\n                    </div>\n                </div>\n\n            </div>\n        </>\n    )\n}\n\nexport default AllSongs;\n","/home/luke/WSL/AppAcademy/dotify/frontend/src/store/currentSong.js",[],"/home/luke/WSL/AppAcademy/dotify/frontend/src/components/LikedSongs/index.js",["166"],"import React from 'react'\nimport { useSelector, useDispatch } from 'react-redux';\nimport { useEffect} from 'react'\nimport * as songActions from \"../../store/songs\"\nimport * as likeActions from \"../../store/likes\"\nimport * as likesActions from \"../../store/likes\"\nimport * as likedSongsActions from \"../../store/likedSongs\"\nimport \"../AllSongs/AllSongs.css\"\nimport \"./LikedSongs.css\"\n\n\n\nfunction LikedSongs() {\n\n    const dispatch = useDispatch();\n    const sessionUser = useSelector((state) => state.session.user);\n    const likedSongs = useSelector((state) => Object.values(state.likedSongs));\n    const userId = sessionUser.id\n\n    useEffect(() => {\n        dispatch(songActions.getSongs())\n        dispatch(likesActions.getLikes())\n        dispatch(likedSongsActions.getLikedSongs(userId))\n    }, [dispatch])\n\n    function unLikeSong(songId) {\n        dispatch(likeActions.deleteLikeStore({ userId, songId }))\n        dispatch(likesActions.getLikes())\n        dispatch(likedSongsActions.getLikedSongs(userId))\n    }\n    function deleteSong(songId) {\n        dispatch(songActions.deleteSongStore(songId))\n        dispatch(likedSongsActions.getLikedSongs(userId))\n    }\n\n\n    return (\n        <>\n            <div className=\"container\">\n                <div className=\"music\">\n\n                    <div className=\"all-songs\">\n                        {likedSongs.map(song =>\n                        (<li className=\"song-lists\" key={song.id}>\n\n                            <h2 className=\"song-title\">{song.name}</h2>\n                            <p className=\"song-artist\">{song.artist}</p>\n                            <audio controls className=\"music-controls\">\n                                <source src={song.filePath} type=\"audio/mp3\" />\n                            </audio>\n                            <div className=\"options\">\n\n                                <button className=\"song-button\" value={song.id} onClick={() => unLikeSong(song.id)}>\n                                    <i className=\"fas fa-heart unLike song-icon\" value={song.id}></i>\n                                </button>\n                                <button className=\"song-button\" value={song.id} onClick={() => deleteSong(song.id)}>\n                                    <i className=\"fas fa-times removeSong song-icon\" value={song.id} ></i>\n                                </button>\n                            </div>\n\n                        </li>))\n                        }\n                    </div>\n                </div>\n\n            </div>\n        </>\n    )\n}\nexport default LikedSongs;\n","/home/luke/WSL/AppAcademy/dotify/frontend/src/store/likes.js",[],"/home/luke/WSL/AppAcademy/dotify/frontend/src/components/UploadSongModal/UploadSong.js",["167"],"\nimport React, { useState } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { useHistory } from \"react-router-dom\";\nimport * as songActions from \"../../store/songs\"\n\nexport function UploadSong() {\n    const dispatch = useDispatch();\n    const sessionUser = useSelector((state) => state.session.user);\n    const [name, setName] = useState(\"\");\n    const [artist, setArtist] = useState(\"\");\n    const [songFile, setSongFile] = useState(null);\n    const [errors, setErrors] = useState([]);\n    const history = useHistory()\n\n    const handleSubmit = (e) => {\n        e.preventDefault();\n        let newErrors = [];\n        const userId = sessionUser.id\n        return dispatch(songActions.createSong({ name, artist, songFile, userId }))\n            .then(() => {\n                setName(\"\");\n                setArtist(\"\");\n                setSongFile(null);\n                dispatch(songActions.getSongs())\n            })\n            .catch((res) => {\n                if (res.data && res.data.errors) {\n                    newErrors = res.data.errors;\n                    setErrors(newErrors);\n                }\n            });\n\n    };\n\n    const updateFile = (e) => {\n        const file = e.target.files[0];\n        if (file) setSongFile(file);\n    };\n\n    return (\n        <div className=\"song-add-container\">\n\n            <form className=\"song-add pure-form-stacked\" onSubmit={handleSubmit}>\n                <ul>\n                    {errors.map((error, idx) => <li key={idx}>{error}</li>)}\n                </ul>\n                <h2 className=\"add-song-title\">Add a Song</h2>\n                <label>\n                    Song Name\n        <input\n                        type=\"text\"\n                        value={name}\n                        onChange={(e) => setName(e.target.value)}\n                        required\n                    />\n                </label>\n                <label>\n                    Artist\n        <input\n                        type=\"text\"\n                        value={artist}\n                        onChange={(e) => setArtist(e.target.value)}\n                        required\n                    />\n                </label>\n                <label>\n                    <input className=\"file\" type=\"file\" onChange={updateFile} />\n                </label>\n                <button className=\"addSong-button\" type=\"submit\">Add Song</button>\n            </form>\n            <div className=\"space\" ></div>\n        </div>\n    );\n}\n","/home/luke/WSL/AppAcademy/dotify/frontend/src/components/UploadSongModal/index.js",[],"/home/luke/WSL/AppAcademy/dotify/frontend/src/store/likedSongs.js",[],"/home/luke/WSL/AppAcademy/dotify/frontend/src/components/Login/index.js",[],{"ruleId":"168","replacedBy":"169"},{"ruleId":"170","replacedBy":"171"},{"ruleId":"168","replacedBy":"172"},{"ruleId":"170","replacedBy":"173"},{"ruleId":"168","replacedBy":"174"},{"ruleId":"170","replacedBy":"175"},{"ruleId":"176","severity":1,"message":"177","line":22,"column":11,"nodeType":"178","messageId":"179","endLine":22,"endColumn":14},{"ruleId":"176","severity":1,"message":"177","line":48,"column":11,"nodeType":"178","messageId":"179","endLine":48,"endColumn":14},{"ruleId":"176","severity":1,"message":"180","line":3,"column":21,"nodeType":"178","messageId":"179","endLine":3,"endColumn":29},{"ruleId":"176","severity":1,"message":"181","line":6,"column":13,"nodeType":"178","messageId":"179","endLine":6,"endColumn":31},{"ruleId":"176","severity":1,"message":"182","line":10,"column":10,"nodeType":"178","messageId":"179","endLine":10,"endColumn":17},{"ruleId":"183","severity":1,"message":"184","line":26,"column":8,"nodeType":"185","endLine":26,"endColumn":18,"suggestions":"186"},{"ruleId":"183","severity":1,"message":"184","line":24,"column":8,"nodeType":"185","endLine":24,"endColumn":18,"suggestions":"187"},{"ruleId":"176","severity":1,"message":"188","line":14,"column":11,"no