e_5/week_16/projects/authenticate-me/frontend/src/index.js",[],["146","147"],"/home/luke/WSL/AppAcademy/module_5/week_16/projects/authenticate-me/frontend/src/App.js",[],"/home/luke/WSL/AppAcademy/module_5/week_16/projects/authenticate-me/frontend/src/store/index.js",[],"/home/luke/WSL/AppAcademy/module_5/week_16/projects/authenticate-me/frontend/src/store/csrf.js",[],"/home/luke/WSL/AppAcademy/module_5/week_16/projects/authenticate-me/frontend/src/store/session.js",[],"/home/luke/WSL/AppAcademy/module_5/week_16/projects/authenticate-me/frontend/src/components/SignupFormPage/index.js",[],"/home/luke/WSL/AppAcademy/module_5/week_16/projects/authenticate-me/frontend/src/components/Navigation/index.js",[],"/home/luke/WSL/AppAcademy/module_5/week_16/projects/authenticate-me/frontend/src/components/Navigation/ProfileButton.js",[],"/home/luke/WSL/AppAcademy/module_5/week_16/projects/authenticate-me/frontend/src/components/LoginFormModal/index.js",[],"/home/luke/WSL/AppAcademy/module_5/week_16/projects/authenticate-me/frontend/src/components/LoginFormModal/LoginForm.js",[],"/home/luke/WSL/AppAcademy/module_5/week_16/projects/authenticate-me/frontend/src/context/Modal.js",[],"/home/luke/WSL/dotify/frontend/src/index.js",[],["148","149"],"/home/luke/WSL/dotify/frontend/src/App.js",["150","151"],"/home/luke/WSL/dotify/frontend/src/store/session.js",[],"/home/luke/WSL/dotify/frontend/src/store/csrf.js",[],"/home/luke/WSL/dotify/frontend/src/context/Modal.js",[],"/home/luke/WSL/dotify/frontend/src/store/index.js",[],"/home/luke/WSL/dotify/frontend/src/store/songs.js",["152"],"import { fetch } from './csrf';\nconst SET_SONGS = 'songs/SET_SONGS';\nconst REMOVE_SONG = 'songs/REMOVE_SONG'\nconst ADD_SONG = 'songs/ADD_SONG'\n\n//action creater that sets the song in the song slice of state\nexport const setSongs = (songs) => {\n    return {\n        type: SET_SONGS,\n        payload: songs,\n    };\n};\n\nexport const removeSong = (songId) => {\n    return {\n        type: REMOVE_SONG,\n        payload: songId,\n    };\n}\n\nexport const addSong = (song) => {\n    return {\n        type: ADD_SONG,\n        payload: song,\n    };\n}\n\n\nexport const deleteSongStore = (songId) => async (dispatch)=>{\n    const res = await fetch(`api/songs/${songId}`,{\n        method: \"DELETE\"\n    })\n    dispatch(removeSong(songId))\n}\n\n\n\n//thunk which accepts an object of key value pairs and turns them into FormData entries to send with your request.\nexport const createSong = (song) => async (dispatch) => {\n    const { name, artist, songFile, userId } = song; //can add songs entry if multiple\n\n    const formData = new FormData();\n    formData.append(\"name\", name);\n    formData.append(\"artist\", artist);\n    formData.append(\"songFile\", songFile);\n    formData.append(\"userId\", userId);\n\n    // for multiple files\n    // if (images && images.length !== 0) {\n    //     for (var i = 0; i < images.length; i++) {\n    //         formData.append(\"images\", images[i]);\n    //     }\n    // }\n\n    // for single file\n    const res = await fetch(`/api/songs/`, {\n        headers: {\n            \"Content-Type\": \"multipart/form-data\",\n        },\n        method: \"POST\",\n        body: formData,\n    });\n\n    const newSong = res.data.song\n    dispatch(addSong(newSong))\n};\n\n\n\n\n//this is the thunk that\n// 1) gets songs from route\n// 2) normalizes song data\n// 3) updates state\n\nexport const getSongs = () => async dispatch => {\n    const res = await fetch('/api/songs');\n    const songs = res.data.songs\n    let normalizedSongs = {}\n    for (let i =0; i<songs.length; i++){\n        const song = songs[i]\n        normalizedSongs[song.id] = song\n    }\n    dispatch(setSongs(normalizedSongs))\n};\n\nconst initialState = {};\n\n//holds the current songs state information\nexport const songsReducer = (state = initialState, action) => {\n    let newState;\n    switch (action.type) {\n        case SET_SONGS:\n            newState = Object.assign({}, state, action.payload);\n            return newState;\n        case ADD_SONG:\n            newState = {...state}\n            newState[action.payload.id] = action.payload\n            return newState;\n        case REMOVE_SONG:\n            newState = { ...state }\n            delete newState[action.payload]\n            return newState;\n        default:\n            return state;\n    }\n};\n","/home/luke/WSL/dotify/frontend/src/store/likedSongs.js",[],"/home/luke/WSL/dotify/frontend/src/store/currentSong.js",[],"/home/luke/WSL/dotify/frontend/src/store/likes.js",[],"/home/luke/WSL/dotify/frontend/src/components/Navigation/index.js",[],"/home/luke/WSL/dotify/frontend/src/components/LikedSongs/index.js",["153"],"import React from 'react'\nimport { useSelector, useDispatch } from 'react-redux';\nimport { useEffect} from 'react'\nimport * as songActions from \"../../store/songs\"\nimport * as likeActions from \"../../store/likes\"\nimport * as likesActions from \"../../store/likes\"\nimport * as likedSongsActions from \"../../store/likedSongs\"\nimport \"../AllSongs/AllSongs.css\"\nimport \"./LikedSongs.css\"\n\n\n\nfunction LikedSongs() {\n\n    const dispatch = useDispatch();\n    const sessionUser = useSelector((state) => state.session.user);\n    const likedSongs = useSelector((state) => Object.values(state.likedSongs));\n    const userId = sessionUser.id\n\n    useEffect(() => {\n        dispatch(songActions.getSongs())\n        dispatch(likesActions.getLikes())\n        dispatch(likedSongsActions.getLikedSongs(userId))\n    }, [dispatch])\n\n    function unLikeSong(songId) {\n        dispatch(likeActions.deleteLikeStore({ userId, songId }))\n        dispatch(likesActions.getLikes())\n        dispatch(likedSongsActions.getLikedSongs(userId))\n    }\n    function deleteSong(songId) {\n        dispatch(songActions.deleteSongStore(songId))\n        dispatch(likedSongsActions.getLikedSongs(userId))\n    }\n\n\n    return (\n        <>\n            <div className=\"container\">\n                <div className=\"music\">\n\n                    <div className=\"all-songs\">\n                        {likedSongs.map(song =>\n                        (<li className=\"song-lists\" key={song.id}>\n\n                            <h2 className=\"song-title\">{song.name}</h2>\n                            <p className=\"song-artist\">{song.artist}</p>\n                            <audio controls className=\"music-controls\">\n                                <source src={song.filePath} type=\"audio/mp3\" />\n                            </audio>\n                            <div className=\"options\">\n\n                                <button className=\"song-button\" value={song.id} onClick={() => unLikeSong(song.id)}>\n                                    <i className=\"fas fa-heart unLike song-icon\" value={song.id}></i>\n                                </button>\n                                <button className=\"song-button\" value={song.id} onClick={() => deleteSong(song.id)}>\n                                    <i className=\"fas fa-times removeSong song-icon\" value={song.id} ></i>\n                                </button>\n                            </div>\n\n                        </li>))\n                        }\n                    </div>\n                </div>\n\n            </div>\n        </>\n    )\n}\nexport default LikedSongs;\n","/home/luke/WSL/dotify/frontend/src/components/AllSongs/index.js",["154","155","156","157"],"import React from 'react'\nimport { useSelector, useDispatch } from 'react-redux';\nimport { useEffect, useState } from 'react'\nimport * as songActions from \"../../store/songs\"\nimport * as likeActions from \"../../store/likes\"\nimport * as currentSongActions from \"../../store/currentSong\"\nimport * as likesActions from \"../../store/likes\"\nimport * as likedSongsActions from \"../../store/likedSongs\"\nimport \"./AllSongs.css\"\nimport { NavLink } from 'react-router-dom'\n\n\n\nfunction AllSongs() {\n\n    const dispatch = useDispatch();\n\n    const songs = useSelector((state) => Object.values(state.songs));\n    const sessionUser = useSelector((state) => state.session.user);\n    const userId = sessionUser.id\n\n    useEffect(() => {\n        dispatch(songActions.getSongs())\n        dispatch(likesActions.getLikes())\n        dispatch(likedSongsActions.getLikedSongs(userId))\n    }, [dispatch])\n\n\n    function likeSong(songId) {\n        dispatch(likeActions.createLike({ userId, songId }))\n        dispatch(likesActions.getLikes())\n    }\n    function unLikeSong(songId) {\n        dispatch(likeActions.deleteLikeStore({ userId, songId }))\n        dispatch(likesActions.getLikes())\n    }\n    function deleteSong(songId) {\n        dispatch(songActions.deleteSongStore(songId))\n    }\n\n\n    return (\n        <>\n            <div className=\"container\">\n                <div className=\"music\">\n\n                    <div className=\"all-songs\">\n                        {songs.map(song =>\n                        (<li className=\"song-lists\" key={song.id}>\n\n                            <h2 className=\"song-title\">{song.name}</h2>\n                            <p className=\"song-artist\">{song.artist}</p>\n                            <audio controls className=\"music-controls\">\n                                <source src={song.filePath} type=\"audio/mp3\" />\n                            </audio>\n                            <div className=\"options\">\n                                <button className=\"song-button\" value={song.id} onClick={() => likeSong(song.id)}>\n                                    <i className=\"fas fa-heart Like song-icon\" value={song.id}></i>\n                                </button>\n                                <button className=\"song-button\" value={song.id} onClick={() => unLikeSong(song.id)}>\n                                    <i className=\"fas fa-heart unLike song-icon\" value={song.id}></i>\n                                </button>\n                                <button className=\"song-button\" value={song.id} onClick={() => deleteSong(song.id)}>\n                                    <i className=\"fas fa-times removeSong song-icon\" value={song.id} ></i>\n                                </button>\n                            </div>\n\n                        </li>))\n                        }\n                    </div>\n                </div>\n\n            </div>\n        </>\n    )\n}\n\nexport default AllSongs;\n","/home/luke/WSL/dotify/frontend/src/components/UploadSongModal/index.js",["158"],"\nimport React, { useState } from 'react';\nimport { Modal } from '../../context/Modal';\nimport { UploadSong } from './UploadSong';\nimport { NavLink } from 'react-router-dom'\nimport \"./UploadSongForm.css\"\n\nexport default function UploadSongModal() {\n    const [showModal, setShowModal] = useState(false);\n\n    return (\n        <>\n            <span className=\"link\" onClick={() => setShowModal(true)}><i className=\"fas fa-upload\"></i>Upload Song</span>\n            {showModal && (\n                <Modal className=\"add-song-modal\" onClose={() => setShowModal(false)}>\n                    <UploadSong showModal={showModal} setShowModal={setShowModal}/>\n                </Modal>\n            )}\n        </>\n    );\n}\n","/home/luke/WSL/dotify/frontend/src/components/Login/index.js",[],"/home/luke/WSL/dotify/frontend/src/components/Sidebar/index.js",[],"/home/luke/WSL/dotify/frontend/src/components/MusicBar/index.js",[],"/home/luke/WSL/dotify/frontend/src/components/Navigation/ProfileButton.js",[],"/home/luke/WSL/dotify/frontend/src/components/UploadSongModal/UploadSong.js",["159"],"\nimport React, { useState } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { useHistory } from \"react-router-dom\";\nimport * as songActions from \"../../store/songs\"\n\nexport function UploadSong({ showModal, setShowModal }) {\n    const dispatch = useDispatch();\n    const sessionUser = useSelector((state) => state.session.user);\n    const [name, setName] = useState(\"\");\n    const [artist, setArtist] = useState(\"\");\n    const [songFile, setSongFile] = useState(null);\n    const [errors, setErrors] = useState([]);\n    const [loading, setLoading] = useState(false)\n    const history = useHistory()\n\n    const handleSubmit = async(e) => {\n        e.preventDefault();\n        let newErrors = [];\n        const userId = sessionUser.id\n        try {\n            await setLoading(true)\n            await dispatch(songActions.createSong({ name, artist, songFile, userId }))\n            await setName(\"\");\n            await setArtist(\"\");\n            await setSongFile(null);\n            await history.push(\"/\")\n            await setShowModal(false)\n\n        } catch (errors) {\n            setErrors(errors);\n            console.log(\"errors\", errors)\n        }\n\n    };\n\n    const updateFile = (e) => {\n        const file = e.target.files[0];\n        if (file) setSongFile(file);\n    };\n\n    return (\n        <div className=\"song-add-container\">\n\n\n            <form className=\"song-add pure-form-stacked\" onSubmit={handleSubmit}>\n                <ul>\n                    {errors.map((error, idx) => <li key={idx}>{error}</li>)}\n                </ul>\n                <h2 className=\"add-song-title\">Add a Song</h2>\n                <label>\n                    Song Name\n        <input\n                        type=\"text\"\n                        value={name}\n                        onChange={(e) => setName(e.target.value)}\n                        required\n                    />\n                </label>\n                <label>\n                    Artist\n        <input\n                        type=\"text\"\n                        value={artist}\n                        onChange={(e) => setArtist(e.target.value)}\n                        required\n                    />\n                </label>\n                <label>\n                    <input className=\"file\" type=\"file\" onChange={updateFile} />\n                </label>\n                {!loading ? <button className=\"addSong-button\" type=\"submit\">Add Song</button>:\n                <div className=\"loader\"></div>\n                }\n\n\n            </form>\n            <div className=\"space\" ></div>\n        </div>\n    );\n}\n","/home/luke/WSL/dotify/frontend/src/components/LoginFormModal/index.js",[],"/home/luke/WSL/dotify/frontend/src/components/SignupFormModal/index.js",[],"/home/luke/WSL/dotify/frontend/src/components/LoginFormModal/LoginForm.js",[],"/home/luke/WSL/dotify/frontend/src/components/SignupFormModal/SignupForm.js",[],{"ruleId":"160","replacedBy":"161"},{"ruleId":"162","replacedBy":"163"},{"ruleId":"160","replacedBy":"164"},{"ruleId":"162","replacedBy":"165"},{"ruleId":"166","severity":1,"message":"167","line":16,"column":10,"nodeType":"168","messageId":"169","endLine":16,"endColumn":19},{"ruleId":"166","severity":1,"message":"170","line":16,"column":21,"nodeType":"168","messageId":"169","endLine":16,"endColumn":33},{"ruleId":"166","severity":1,"message":"171","line":30,"column":11,"nodeType":"168","messageId":"169","endLine":30,"endColumn":14},{"ruleId":"172","severity":1,"message":"173","line":24,"column":8,"nodeType":"174","endLine":24,"endColumn":18,"suggestions":"175"},{"ruleId":"166","severity":1,"message":"176","line":3,"column":21,"nodeType":"168","messageId":"169","endLine":3,"endColumn":29},{"ruleId":"166","severity":1,"message":"177","line":6,"column":13,"nodeType":"168","messageId":"169","endLine":6,"endColumn":31},{"ruleId":"166","severity":1,"message":"178","line":10,"column":10,"nodeType":"168","messageId":"169","endLine":10,"endColumn":17},{"ruleId":"172","severity":1,"message":"173","line":26,"column":8,"nodeType":"174","endLine":26,"endColumn":18,"suggestions":"179"},{"ruleId":"166","severity":1,"message":"1